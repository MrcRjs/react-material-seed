{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "modules": true,
      "experimentalObjectRestSpread": true,
      "jsx": true
    }
  },
  "env": {
      "browser": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "react",
    "eslint:recommended",
    "plugin:react/recommended",
  ],
  "react/jsx-indent-props": [
      2,
      2
  ],
}
